{
    "compileOnSave": false,
    "compilerOptions": {
        "baseUrl": "./",
        "disableSourceOfProjectReferenceRedirect": true,
        "outDir": "./dist/out-tsc",
        "forceConsistentCasingInFileNames": true,
        "strict": false,
        "noImplicitOverride": true,
        "noPropertyAccessFromIndexSignature": false,
        "noImplicitReturns": false,
        "noFallthroughCasesInSwitch": true,
        "allowSyntheticDefaultImports": true,
        "sourceMap": true,
        "declaration": true,
        "downlevelIteration": true,
        "experimentalDecorators": true,
        "moduleResolution": "node",
        "importHelpers": true,
        "target": "ES2022",
        "module": "ES2022",
        "skipLibCheck": true,
        "lib": ["ES2022", "DOM"],
        "typeRoots": ["./node_modules/@types", "./@types"],
        "types": ["node"],
        "useDefineForClassFields": false,
        "paths": {
            "stream": ["./node_modules/stream-browserify"],
            "crypto": ["./node_modules/crypto-browserify"],
            "assert": ["./node_modules/assert"],
            "http": ["./node_modules/stream-http"],
            "https": ["./node_modules/https-browserify"],
            "os": ["./node_modules/os-browserify"],
            "url": ["./node_modules/url"],
            "zlib": ["./node_modules/browserify-zlib"],
            "events": ["./node_modules/events"],
            "wirejs": ["../wirejs/src/index"],
            "app/*": ["./src/app/*.ts"],
            "_services/*": ["./src/_services/*.ts"],
            "_modals/*": ["./src/_modals/*.ts"],
            "_types/*": ["./src/_types/*.ts"],
            "_guards/*": ["./src/_guards/*.ts"],
            "_components/*": ["./src/_components/*.ts"],
            "environments/*": ["./src/environments/*.ts"]
        },
        "noImplicitAny": false,
        "composite": true,
    },
    "include": ["./src/**/*.ts"],
    "angularCompilerOptions": {
        "enableI18nLegacyMessageIdFormat": false,
        "strictInjectionParameters": true,
        "strictInputAccessModifiers": true,
        "strictTemplates": true
    }
}